{
  "name": "apigateway",
  "version": "1.0.0",
  "description": "O API Gateway nada mais é do que um gerenciador de tráfego que faz a interface com o serviço de back-end real ou de dados.  Em seguida, aplica políticas, autenticação e controle de acesso geral para chamadas de APIs, com o objetivo de proteger dados sigilosos e importantes.",
  "main": "index.js",
  "scripts": {
    "start": "nodemon",
    "start:dev": "nodemon",
    "build": "npx tsc",
    "migration:generate": "typeorm-ts-node-commonjs -d ./src/data-source.ts migration:generate ./src/migrations/default",
    "migration:run": "typeorm-ts-node-commonjs -d ./src/data-source.ts migration:run"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/node-fetch": "^2.6.4",
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-list-endpoints": "^6.0.0",
    "firebase": "^9.22.2",
    "firebase-admin": "^11.9.0",
    "google-auth-library": "^8.8.0",
    "googleapis": "^118.0.0",
    "node-fetch": "^2.6.11",
    "pg": "^8.11.0",
    "reflect-metadata": "^0.1.13",
    "xmlhttprequest-ts": "^1.0.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/body-parser": "^1.19.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/express-list-endpoints": "^6.0.0",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^20.2.4",
    "body-parser": "^1.20.2",
    "jsonwebtoken": "^9.0.0",
    "nodemon": "^2.0.22",
    "sucrase": "^3.32.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typeorm": "^0.3.16",
    "typescript": "^5.0.4"
  }
}
